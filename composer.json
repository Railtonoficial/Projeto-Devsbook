{
    "require": {
        "clancats/hydrahon": "^1.1",
        "vlucas/phpdotenv": "^5.5"
    },
    "autoload": {
        "psr-4": {
            "core\\": "core/",
            "src\\": "src/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "composer dump-autoload"
        ],
        "post-update-cmd": [
            "composer dump-autoload"
        ]
    }
}
